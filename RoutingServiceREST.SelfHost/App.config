<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
		<!-- Mutăm aici cheia folosită în RoutingServiceREST (ConfigurationManager.AppSettings) -->
		<add key="JCDECAUX_API_KEY" value="13f2c4be0f2297fac09d629d0ca3bebec76b9627" />
	</appSettings>

	<system.serviceModel>
		<!-- AICI rămân DOAR lucrurile de client pentru ProxyCacheService -->
