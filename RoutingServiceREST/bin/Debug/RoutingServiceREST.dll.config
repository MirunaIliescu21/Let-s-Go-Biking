<configuration>
	<appSettings>
		<add key="JCDECAUX_API_KEY" value="13f2c4be0f2297fac09d629d0ca3bebec76b9627" />
	</appSettings>
	<system.serviceModel>
		<!-- Adaug un binding foarte mare pentru clientul WCF -->
		<bindings>
			<basicHttpBinding>
				<binding name="LargeProxyBinding"
					 maxBufferSize="2147483647"
					 maxReceivedMessageSize="2147483647"
					 maxBufferPoolSize="2147483647"
					 transferMode="Buffered"
					 messageEncoding="Text">
					<readerQuotas maxDepth="64"
							maxStringContentLength="2147483647"
							maxArrayLength="2147483647"
							maxBytesPerRead="2147483647"
							maxNameTableCharCount="2147483647" />
				</binding>
				
				<binding name="BasicHttpBinding_IProxyService"
						 maxReceivedMessageSize="20000000"
						 maxBufferSize="20000000"
						 maxBufferPoolSize="20000000">
					<readerQuotas maxStringContentLength="20000000"
								  maxArrayLength="20000000"
								  maxBytesPerRead="20000000"
								  maxDepth="64"
								  maxNameTableCharCount="20000000" />
				</binding>
			</basicHttpBinding>

			<webHttpBinding>
				<binding name="LargeWebBinding"
						 maxReceivedMessageSize="20000000"
						 maxBufferSize="20000000"
						 maxBufferPoolSize="20000000">
					<readerQuotas maxStringContentLength="20000000"
								  maxArrayLength="20000000"
								  maxBytesPerRead="20000000"
								  maxDepth="64"
								  maxNameTableCharCount="20000000" />
				</binding>
			</webHttpBinding>
		</bindings>

		<!-- bindingConfiguration="BasicHttpBinding_IProxyService"-->

		<client>
			<endpoint address="http://localhost:8734/Design_Time_Addresses/ProxyCacheService/Service1/"
					  binding="basicHttpBinding"
					  bindingConfiguration="LargeProxyBinding"
					  contract="ProxyRef.IProxyService"
					  name="BasicHttpBinding_IProxyService" />
		</client>

		<!-- The REStT service -->
		<services>
			<service name="RoutingServiceREST.RoutingService">
				<endpoint address=""
						  binding="webHttpBinding"
						  bindingConfiguration="LargeWebBinding"
						  contract="RoutingServiceREST.IRoutingServiceREST"
						  behaviorConfiguration="restBehavior" />
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
				<host>
					<baseAddresses>
						<add baseAddress="http://localhost:8733/Design_Time_Addresses/RoutingServiceREST/Service1/" />
					</baseAddresses>
				</host>
			</service>
		</services>

		<behaviors>
			<endpointBehaviors>
				<behavior name="restBehavior">
					<webHttp helpEnabled="true" defaultOutgoingResponseFormat="Json" />
				</behavior>
			</endpointBehaviors>
			<serviceBehaviors>
				<behavior>
					<serviceMetadata httpGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>
